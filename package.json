{
  "name": "CZ-NL",
  "description": "Czech-Dutch vocabulary tests",
  "version": "1.0.0",
  "scripts": {
    "start": "tsc && concurrently \"npm run watch:ts\" \"nodemon server.js --ignore public/\" ",
    "build:bw": "browserify public/assets/js/browserify_libs.js -o public/assets/js/libs.js",
    "build:ts": "tsc",
    "watch:ts": "tsc -w",
    "browsersync": "browser-sync start --files \"public/**/*.*\" --proxy 127.127.0.1:8080 --reload-delay=300 --reload-debounce=500",
    "postinstall": "typings install",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings",
    "build:prod": "npm run bundle && npm run minify:bundle && npm run minify:libs && npm run minify:css",
    "bundle": "node tools/builder.js",
    "minify:bundle": "uglifyjs public/assets/js/bundle.js --screw-ie8 --compress --mangle --output public/assets/js/bundle.min.js",
    "minify:libs": "uglifyjs public/assets/js/libs.js --screw-ie8 --compress --mangle --output public/assets/js/libs.min.js",
    "minify:css": "cleancss public/assets/css/styles.css -o public/assets/css/styles.min.css"
  },
  "engines": {
    "node": "6.2.2"
  },
  "author": "Kristiaan Van Ermengem",
  "dependencies": {
    "@angular/common": "2.0.0-rc.7",
    "@angular/compiler": "2.0.0-rc.7",
    "@angular/core": "2.0.0-rc.7",
    "@angular/forms": "2.0.0-rc.7",
    "@angular/http": "2.0.0-rc.7",
    "@angular/platform-browser": "2.0.0-rc.7",
    "@angular/platform-browser-dynamic": "2.0.0-rc.7",
    "@angular/router": "3.0.0-rc.3",
    "async": "^2.0.1",
    "body-parser": "^1.15.2",
    "bootstrap": "^3.3.6",
    "compression": "^1.6.2",
    "core-js": "^2.4.1",
    "express": "^4.14.0",
    "font-awesome": "^4.6.3",
    "hbs": "^4.0.0",
    "mongodb": "^2.2.4",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.12",
    "systemjs": "0.19.27",
    "zone.js": "^0.6.21"
  },
  "devDependencies": {
    "browser-sync": "^2.13.0",
    "browserify": "^13.1.0",
    "concurrently": "^2.0.0",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.0",
    "gulp-nodemon": "^2.1.0",
    "lite-server": "^2.2.0",
    "systemjs-builder": "^0.15.25",
    "typescript": "^1.8.10",
    "typings": "^1.0.4"
  }
}
